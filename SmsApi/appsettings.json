{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "using": [
      "Serilog.Sinks.Seq",
      "Serilog.Sinks.RollingFile"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "UseAsync": "true",
        "Args": {
          "pathFormat": "logs\\log-{Date}.txt",
          "fileSizeLimitBytes": 1000000
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "Seq.apiKey": "3lcXMeOWdkGV46OuIfy1"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DatabaseConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=SmsApi;Trusted_Connection=True;ConnectRetryCount=0"
  }
}